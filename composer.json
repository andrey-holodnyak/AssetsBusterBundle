{
    "name": "avholodnyak/assets-buster-bundle",
    "description": "This bundle provides support for cache busting using buster files. It uses custom version strategies feature that was introduced in Symfony 3.1.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Holodnyak",
            "email": "andrey.holodnyak@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AVHolodnyak\\AssetsBusterBundle\\": ""
        }
    },
    "require": {
        "symfony/http-kernel": "^3.1",
        "symfony/asset": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/config": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/framework-bundle": "^3.1",
        "symfony/twig-bundle": "^3.1",
        "symfony/templating": "^3.1",
        "symfony/translation": "^3.1"
    }
}
